[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
	hideDotFiles = dotGitOnly
[remote "origin"]
	url = git@github.com:BryanApellanes/bamvvm.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
	remote = origin
	merge = refs/heads/master
[branch "bamvvm"]
	remote = origin
	merge = refs/heads/bamvvm
[remote "bitbucket"]
	url = git@bitbucket.org:stickerize/bamvvm.git
	fetch = +refs/heads/*:refs/remotes/bitbucket/*
[remote "gitlab"]
	url = git@gitlab.com:bryan.apellanes/bamvvm.git
	fetch = +refs/heads/*:refs/remotes/gitlab/*
[remote "github"]
	url = git@github.com:BryanApellanes/bamvvm.git
	fetch = +refs/heads/*:refs/remotes/github/*
[remote "all"]
	url = git@github.com:BryanApellanes/bamvvm.git
	url = git@bitbucket.org:stickerize/bamvvm.git
	url = git@gitlab.com:bryan.apellanes/bamvvm.git
