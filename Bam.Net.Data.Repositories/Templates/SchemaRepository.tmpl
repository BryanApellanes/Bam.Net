using System;
using System.Collections;
using System.Collections.Generic;
using System.Reflection;
using System.Data;
using System.Data.Common;
using System.Linq;
using System.Threading.Tasks;
using Bam.Net;
using Bam.Net.Data;
using @(Model.SourceNamespace);

namespace @(Model.DestinationNamespace)
{
	[Serializable]
	public class @(Model.SchemaName)Repository: DaoRepository
	{
		public @(Model.SchemaName)Repository()
		{
			SchemaName = "@(Model.SchemaName)";
			@foreach(Type type in Model.Types)
			{
				WriteAddType(type);
			}

		}

		@foreach(Type type in Model.Types)
		{
			WriteMethods(type);
		}

	}
}																								
