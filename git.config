[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
	hideDotFiles = dotGitOnly
[remote "origin"]
	url = git@github.com:BryanApellanes/Bam.Net.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
	remote = github
	merge = refs/heads/master
[submodule "BamContent"]
	url = git@github.com:BryanApellanes/BamContent.git
[branch "convert-to-portable-libraries"]
	remote = github
	merge = refs/heads/convert-to-portable-libraries
[remote "bitbucket"]
	url = git@bitbucket.org:bryanapellanes/bam.net.git
	fetch = +refs/heads/*:refs/remotes/bitbucket/*
[remote "github"]
	url = git@github.com:BryanApellanes/Bam.Net.git
	fetch = +refs/heads/*:refs/remotes/github/*
